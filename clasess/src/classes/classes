public class Wyjazd {
    private Long id;
    private LocalDateTime date;
    private String nazwa;
    private String opis;
    private List<Zuch> uczestnicy;
    private Uzytkownik prowadzacy;
}

public class Wplata {
    private Long id;
    private LocalDate date;
    private float kwota;
}

public class Obecnosc {
    private Long id;
    private LocalDate date;
    private boolean obecnosc;
}

public class InformacjaOgolna {
    private Long id;
    private LocalDateTime date;
    private String text;
}

public class Sprawnosc {
    private Long id;

}


public class Zuch extends Uzytkownik{
    private int wiek;
    private List<Rodzic> rodzice;
    private List<Wplata> wplaty;
    private String gruppa;
}



public class Uzytkownik {
    private Long id;
    private String imie;
    private String nazwisko;
    private TypUzytkownika typUzytkownika;
    private String login;
    private String password;
    public String email;
}


public enum TypUzytkownika {
    ADMINISTRATOR,
    ZUCH,
    RODZIC,
    PRZYBOCZNY;
}


public class Rodzic extends Uzytkownik{
    private List<Zuch> dzieci;
}

public class Przyboczny extends Uzytkownik{
}

public class Administarator extends Uzytkownik{
}

